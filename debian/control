 guacharo-l10n-gd,
 guacharo-l10n-gl,
 guacharo-l10n-he,
 guacharo-l10n-hu,
 guacharo-l10n-id,
 guacharo-l10n-is,
 guacharo-l10n-it,
 guacharo-l10n-ja,
 guacharo-l10n-ko,
 guacharo-l10n-lt,
 guacharo-l10n-nb-no,
 guacharo-l10n-nl,
 guacharo-l10n-nn-no,
 guacharo-l10n-pa-in,
 guacharo-l10n-pl,
 guacharo-l10n-pt-br,
